<div class="ui grid">
  <div class="left floated fourteen wide column">
    <h2>申请信息</h2>
  </div>
</div>
<%= render 'search_form'  %>
<div class="ui blue segment">
  <div class="ui grid">
    <div class="left floated wide column">
      <%= link_to new_web_written_apply_path, remote: true, class: 'ui primary button' do %>
        <i class="plus icon"></i>新增笔试申请
      <% end %>
    </div>
  </div>
  <table class="ui selectable striped celled table">
    <thead>
    <tr >
      <th class="center aligned">学生ID</th>
      <th class="center aligned">申请ID</th>
      <th class="center aligned">名字</th>
      <th class="center aligned">车号</th>
      <th class="center aligned">申请状态</th>
      <th class="center aligned">操作</th>
    </tr>
    </thead>
    <tbody>
    <%= render partial: 'list', collection: @written_applies, as: :written_apply %>
    </tbody>
  </table>
  <span class="blue">
    <%= page_entries_info(@written_applies) %>
  </span>
  <%= paginate(@written_applies, :theme => 'admin')%>
</div>