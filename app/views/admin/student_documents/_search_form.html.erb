<% if @q.present? %>
  <div class="ui blue segment">
    <%= simple_form_for @q,  url: admin_student_documents_path, method: :get, wrapper: 'semantic_search_form' do |f| %>
      <div class="ui five column grid">
        <%= f.input 'student_no-like', placeholder: '学号' %>
        <%= f.input 'name-like', placeholder: '姓名' %>
        <div class="column">
          <%= f.submit '搜索', class: 'ui primary button' %>
        </div>
      </div>
    <% end %>
  </div>
<% end %>